@font-face {
  font-family: "iA Writer Quattro";
  font-style: normal;
  font-weight: normal;
  src: url('<%= asset_path("iAWriterQuattroS-Regular.eot") %>') format("embedded-opentype"),
       url('<%= asset_path("iAWriterQuattroS-Regular.woff2") %>') format("woff2"),
       url('<%= asset_path("iAWriterQuattroS-Regular.woff") %>') format("woff");
}

